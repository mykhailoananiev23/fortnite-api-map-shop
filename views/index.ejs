<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= title %></title>
    <meta name="description" content="All Spawn Locations &amp; Battle Pass Weekly Challenges in one place with an interactive map.">
    <meta name="theme-color" content="#2f3136">
    <link rel="apple-touch-icon" type="image/png" sizes="180x180" href="/logo/logo.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/logo/32x32.ico">
    <link rel="icon" type="image/png" sizes="16x16" href="/logo/16x16.ico">
    <link rel="manifest" href="site.webmanifest">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="Fortnite.GG">
    <link rel="stylesheet" href="css/map.css">
    <link rel="stylesheet" href="css/map-lib.css">
    <link rel="stylesheet" href="css/global.css">
    <script type="text/javascript">
      IsLogged = false;
      Url = "/";
    </script>
    <script src="js/global.js" type="text/javascript"></script>
  </head>
  <body class="map lang-en">
    <%- include ('./module/navbar.ejs') %>
    <div id="toggle-sidebar" data-tooltip="" data-tooltip-left="">
      <svg width="18" height="18" viewBox="0 0 24 24">
        <path d="M16.67 0l2.83 2.829-9.339 9.175 9.339 9.167-2.83 2.829-12.17-11.996z"></path>
      </svg>
      <span></span>
    </div>
    <div id="resize-sidebar" data-tooltip="Resize sidebar" data-tooltip-left="">
      <svg width="24" height="24" viewBox="0 0 24 24">
        <path d="M6 13v4l-6-5 6-5v4h3v2h-3zm9-2v2h3v4l6-5-6-5v4h-3zm-4-6v14h2v-14h-2z"></path>
      </svg>
    </div>
    <div id="open-filters">
      <svg width="22" height="22" viewBox="0 0 24 24">
        <path d="M14 12v7.883a.999.999 0 01-1.7.83l-2.015-2.016a.998.998 0 01-.285-.832V12h-.026L4.21 4.622A1 1 0 015 3.007V3h14v.007a1 1 0 01.79 1.616L14.026 12H14z"></path>
      </svg> Filter Spawns &amp; Quests
    </div>
    <div id="close-filters">
      <svg width="22" height="22" viewBox="0 0 24 24" fill="#000">
        <path d="M12,9.172L4.828,2L2,4.828L9.172,12L2,19.172L4.828,22L12,14.828L19.172,22L22,19.172L14.828,12L22,4.828L19.172,2L12,9.172Z"></path>
      </svg> Close
    </div>
    <div id="sidebar">
      <div id="sidebar-top"></div>
    </div>
    <div id="map">
      <div id="contextmenu"></div>
    </div>
    <div id="message"></div>
    <script type="text/javascript">
      L10N = {
        'Spawns': "Spawns",
        'Named Locations': "Named Locations",
        'Week 1': "Week 1",
        'Search': "Search",
        'New': "New",
        'Open map': "Open map",
        'Open sidebar': "Open sidebar",
        'Close sidebar': "Close sidebar",
        'Hide': "Hide",
        'Play Video': "Play Video",
        'Draw on map': "Draw on map",
        'Enter marker text': "Enter marker text",
        'Change color': "Change color",
        'Delete': "Delete",
        'Edit': "Edit",
        'Save my drawing': "Save my drawing",
        'Connection error, please try again later.': "Connection error, please try again later.",
        'Saving...': "Saving...",
        'time': {
          labels: ['Years', 'Months', 'Weeks', 'Days', 'Hours', 'Minutes', 'Seconds'],
          labels1: ['Year', 'Month', 'Week', 'Day', 'Hour', 'Minute', 'Second'],
          compactLabels: ['y', 'm', 'w', 'd', 'h', 'm', 's'],
          whichLabels: null,
        },
      };
    </script>
    <script src="js/map-lib.js" type="text/javascript"></script>
    <script src="https://fortnite.gg/data/spawns.js" type="text/javascript"></script>
    <script src="https://fortnite.gg/data/en.js" type="text/javascript"></script>
    <script src="js/map.js" type="text/javascript"></script>
  </body>
</html>